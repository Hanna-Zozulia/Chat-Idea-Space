{
  "name": "chat",
  "version": "1.0.0",
  "description": "Simple chat using Express, Socket.io, and TypeScript (MVC)",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --watch src --exec ts-node src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "tunnel": "dotenv cross-env ngrok http --authtoken $NGROK_AUTHTOKEN 3000",
    "test": "jest"
  },
  "keywords": [
    "chat",
    "express",
    "socket.io",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "ngrok": "^5.0.0-beta.2",
    "socket.io": "^4.7.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.17",
    "@types/supertest": "^6.0.3",
    "cross-env": "^10.1.0",
    "dotenv-cli": "^11.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "supertest": "^7.2.2",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
